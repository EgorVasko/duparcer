<script type='text/javascript'>
    function populate_dfp_custom_target_params(element_id){
        try {
            dfp_custom_targeting_params = JSON.parse(document.getElementById(element_id).textContent);
            for (var key in dfp_custom_targeting_params)
                window.DFPTargeting[key] = dfp_custom_targeting_params[key];
        } catch(err) {}
    }
</script>


        <script type="text/javascript">
  var laqTimer = 0;
  var laqTimeout;
  window.laquesisCookie = null;
  window.laquesisResults = [];
  window.listenLaquesisCookieChange = function () {
      var cookieValue = (document.cookie).split('laquesis_return=').pop().split(';').shift();
      if (cookieValue.length > 0 && cookieValue.indexOf('=') < 0) {
          if (cookieValue != window.laquesisCookie) {
              window.laquesisCookie = cookieValue;
              window.laquesisResults.push({'newResult': cookieValue});
          }
      }

      laqTimer++;
      laqTimeout = setTimeout(window.listenLaquesisCookieChange, 50);

      if(laqTimer > 1000) {
        clearTimeout(laqTimeout);
      }
  };
  window.listenLaquesisCookieChange();
</script>


