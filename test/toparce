        <script type="text/javascript">

  /*
   * Gets the platform type for tracking events.
   */

	// Get the `route` query params from the url
	function getQueryParamValue(param){
		var routeParam = window.location.search.substring(1).split('&').filter(function(a){ return a.split('=')[0] === param});
		if(routeParam.length){
       /// looks for the first value, assuming that there will only be one query parama of a type.
			return routeParam[0].split('=')[1];
		}
  };

  function getPlatformType () {
      var platform_type = 'desktop';
      if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
          platform_type = 'mobile-html5';
      }
      if (/app-android|app-ios/i.test(navigator.userAgent)) {
          platform_type = navigator.userAgent;
      }
      return platform_type;
  };

  function parseEventName(eventName) {
    if (!eventName) {
      return eventName;
    }
    // PAA Complete events look wierd, so we parse them manually
    if (eventName.indexOf('place-an-ad') >= 0 && eventName.indexOf('complete') >= 0) {
      return 'paa_complete';
    }
    if (eventName.indexOf('paa_selling_options_new') >= 0) {
      return 'paa_selling_options';
    }

    // If any
    if (eventName.indexOf('/') >= 0) {
      return null;
    }
    return eventName;
  }
  function getURLParameterByName(name) {
    var url = decodeURIComponent(window.location.href);
    name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
        results = regex.exec(url);
    try {
        if(results && results.length> 0) {
            return decodeURIComponent(results[1].replace(/\+/g, " "));
        }
        else{
            return ""
        }
    } catch (e){
        return "";
    }
  }

  (function(W, D, dbz, undefined) {
      var ninja_vars = {"resultSetType": "browse", "cL2Id": "140", "search_filters": "{}", "language": "en", "resultSetFormat": "list", "adsPromoted": "[]", "pageCount": "1", "cityName": "dubai", "trackPage": "resultSet", "categoryId": "1203", "cityId": 2, "pageNumber": "1", "cL3Id": "1202", "cL4Id": "1203", "cL1Id": "7", "resultCount": "24", "adsFeatured": "[1203-14269724,1203-14268495,1203-14267275,1203-14251483,1203-14254489,1203-14222566,1203-14244687,1203-14240118]", "platformType": "desktop", "ads": "[1203-14269724,1203-14268495,1203-14267275,1203-14251483,1203-14254489,1203-14222566,1203-14244687,1203-14240118,1203-14252039,1203-14210214,1203-14268497,1203-14264014,1203-14256604,1203-14268391,1203-14265389,1203-14093923,1203-14257181,1203-14260506,1203-14251646,1203-14250771,1203-14249727,1203-14233779,1203-14226207,1203-14186149]"};
      // Send a page-load event if there is
      try {
        var appboyEventName = parseEventName(ninja_vars.trackPage);
        if (typeof appboy !== 'undefined' && appboyEventName) {
          var customProperties = {};

          // If my event is one of these, add some customProperties
          if (appboyEventName === "paa_enter_details" ||
              appboyEventName === "paa_selling_options" ||
              appboyEventName === "paa_checkout_confirm" ||
              appboyEventName === "paa_enter_payment_info" ||
              appboyEventName === "paa_complete") {

            ['cL1Id', 'cL2Id', 'cL3Id', 'cL4Id', 'categoryId', 'cityId'].forEach(function(key){
              customProperties[key] = ninja_vars[key];
            })
          }

          // If its a paa_checkout_confirm event, also send a paa_Complete event
          if (appboyEventName.indexOf('paa_checkout_confirm') >= 0) {
            appboy.logCustomEvent('paa_complete', customProperties);
          }
          appboy.logCustomEvent(appboyEventName, customProperties);

