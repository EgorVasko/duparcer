<script>
window.fbAsyncInit = function() {
    FB.init({
        appId      : 170382057251,
        xfbml      : true,
        version    : 'v2.7'
    });
};

(function(d, s, id){
 var js, fjs = d.getElementsByTagName(s)[0];
 if (d.getElementById(id)) {return;}
 js = d.createElement(s); js.id = id;
 js.src = "//connect.facebook.net/en_US/sdk.js";
 fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));

function checkLoginState () {
    FB.login(function(response) {
        if (response.authResponse) {
            var outerResponse = response;
            FB.api('/me/permissions', function(response) {
                var declined = [];
                for (var i = 0; i < response.data.length; i++) {
                    if (response.data[i].status == 'declined') {
                        declined.push(response.data[i].permission)
                    }
                }
                if (declined.indexOf('email') !== -1) {
                    showReaskPrompt();
                } else {
                    sendToken(outerResponse.authResponse.accessToken);
                }
            });
        }
    }, {scope: 'email,public_profile,user_mobile_phone'});
}

function showReaskPrompt() {
    swal({
        title: "Facebook Login",
        text: "Your email address could not be retrieved. Try again?",
        type: "warning",
        showCancelButton: true,
        confirmButtonColor: "rgb(89, 162, 92)",
        confirmButtonText: "Yes",
        cancelButtonText: "No",
        closeOnConfirm: true,
        closeOnCancel: true
    },
    function(isConfirm) {
        if (isConfirm) {
            reaskEmail()
        } else {
            window.location.href = '/accounts/register/'
        }
    });
}

function reaskEmail() {
    FB.login(
        function(response) {
            if (response.authResponse) {
                sendToken(response.authResponse.accessToken);
            }
        },
        {
            scope: 'email',
            auth_type: 'rerequest'
        }
    )
}

function chooseEmailModal (response) {
    var response = JSON.parse(response)
    swal({
        title: 'Choose which email you want to connect to Facebook',
        text: '<form id="chosenEmailForm"><label for="chosen_email1">' + response.choices[0] + '</label><input style="display:inline;width:14px;height:14px;" name="chosen_email" id="chosen_email1" type="radio" value="' + response.choices[0] +  '"/><br /><label for="chosen_email2">' + response.choices[1] + '</label><input style="display:inline;width:14px;height:14px;" name="chosen_email" id="chosen_email2" type="radio" value="' + response.choices[1] + '"/></form>',
        html: true
    }, function() {
        var chosenEmail = $("#chosenEmailForm input[type='radio']:checked").val()
        sendToken(response.token, chosenEmail)
    })
}

function getQueryParam(param) {
    var result =  window.location.search.match(
        new RegExp("(\\?|&)" + param + "(\\[\\])?=([^&]*)")
    );

    return result ? result[3] : undefined;
}

var removeQueryFields = function (url) {
    var fields = [].slice.call(arguments, 1).join('|'),
        parts = url.split( new RegExp('[&?](' + fields + ')=[^&]*') ),
        length = parts.length - 1;
    return parts[0] + '?' + (length ? parts[length].slice(1) : '');
}

function getNextUrl() {
    var url = getQueryParam('next'),
        other_query_params = removeQueryFields(window.location.search, 'next');
    if(url) {
        url = window.location.origin + url;
    }

    if(url && other_query_params) {
        url = url + other_query_params;
    }

    console.log(url);
    return url;
}

function sendToken(token, chosenEmail) {
    var xmlhttp;
    if (window.XMLHttpRequest) {
        xmlhttp = new XMLHttpRequest();
    } else {
        xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
    }
    xmlhttp.onreadystatechange = function() {
        if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
            trackOptimizelyforLogin('facebook_login_register_success')
            trackOptimizelyforLogin('overall_login_page_success')
            var next = getNextUrl();
            if (next) {
                window.location.replace(next);
            } else {
                window.location.reload();
            }
        } else if (xmlhttp.readyState == 4 && xmlhttp.status == 201) {
            trackOptimizelyforLogin('facebook_login_register_success')
            trackOptimizelyforLogin('overall_login_page_success')
            window.location.reload();
        } else if (xmlhttp.readyState == 4 && xmlhttp.status == 409) {
            chooseEmailModal(xmlhttp.responseText);
        } else if (xmlhttp.readyState == 4 && xmlhttp.status == 403) {
            window.location.reload()
        } else if (xmlhttp.readyState == 4 && xmlhttp.status == 406) {
            swal("Error", xmlhttp.responseText, "error");
        } else if (xmlhttp.readyState == 4 && xmlhttp.status == 428) {
            swal("Uh oh!", "This Facebook account is already connected with another account", "error")
        }
    };
    xmlhttp.open(
        'GET',
        '/profile/login/facebook/?access_token=' + token +
        (chosenEmail ? '&chosen_email=' + encodeURIComponent(chosenEmail) : ''),
        true
    );
    xmlhttp.send();
}

function trackOptimizelyforLogin(tag) {
    window['optimizely'] = window['optimizely'] || [];
    // sends a tracking call to Optimizely for the given event name.
    window.optimizely.push(["trackEvent", tag]);
}

</script>


