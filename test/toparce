    (function (window) {

        window.parseInt4 = function parseInt4(num) {
            if(parseInt(num) > 2000000000){
                return 2000000000;
            }else if(parseInt(num) < 0){
                return 0;
            }else{
                return parseInt(num);
            }
        };

