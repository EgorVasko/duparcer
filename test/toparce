        <script type="text/javascript">
            var getSearchParams = function(items){
                var keywords = "";
                var eventContextData = {
                    listings: items,
                    isSearch: 0
                }
                if("None" !== "None") eventContextData['userId'] = None;
                if(location.search){
                    eventContextData["isSearch"] = 1;
                    var searchInputs = document.getElementById("search-content").querySelectorAll("input, select");
                    var schemaParamsArr = ["rc", "c1", "c2", "site", "s", "price__gte", "price__lte", "bedrooms__gte","bedrooms__lte", "places__id__in", "keywords"];
                    [].forEach.call(searchInputs, function(el) {
                        if(schemaParamsArr.indexOf(el.name) > -1 && el.value){
                            eventContextData[el.name] = el.value;
                        }
                        if(el.name == "places__id__in"){
                            location.search.split("&").forEach(function(str){
                                if(str.split("=")[0] == "places__id__in" && str.split("=")[1]){
                                    eventContextData["places__id__in"] = decodeURIComponent(str.split("=")[1]).split(",").splice(0, decodeURIComponent(str.split("=")[1]).split(",").length-1);
                                }
                            });
                        }
                    });
                    var propsStr= JSON.stringify(eventContextData);
                    var eventContextData = JSON.parse(propsStr, function(prop, value) {
                        try{
                            switch(prop) {
                                case "site":
                                    this.cityId = window.parseInt4(value);
                                    return;
                                case "s":
                                    this.section = value;
                                    break;
                                case "price__gte":
                                    this.priceGTE = window.parseInt4(value);
                                    break;
                                case "price__lte":
                                    this.priceLTE = window.parseInt4(value);
                                    break;
                                case "bedrooms__gte":
                                    this.bedroomsGTE = window.parseInt4(value);
                                    break;
                                case "bedrooms__lte":
                                    this.bedroomsLTE = window.parseInt4(value);
                                    break;
                                case "places__id__in":
                                    this.places = value.join(',').split(',').map(Number);
                                    break;
                                default:
                                    return value;
                            }
                        }catch(e){
                            return value;
                        }
                    });
                    eventContextData["rc"] = parseInt(eventContextData["rc"]);
                    eventContextData["c1"] = parseInt(eventContextData["c1"]);
                    eventContextData["c2"] = parseInt(eventContextData["c2"]);

                    for ( key in eventContextData){
                        if(!eventContextData[key] || eventContextData[key].length == 0){
                            delete eventContextData[key]
                        }
                    }
                }
                window.snowplow('trackPageView', null, {
                    schema: "iglu:com.dubizzle/list_page_view/jsonschema/1-0-0",
                    data: eventContextData
                });
            }
