})();


        $(document).ready(function(){
            $(".preventDefaultLink").click(function(e){
                e.preventDefault();
            })
            $(".facts").each(function(i, item){
                if ($(".facts").eq(i).find("div").eq(0).text().indexOf("Studio") > -1){$(".facts").eq(i).find("div").eq(0).text("Studio")}
            })
            $(".card.one-item.new-card-view").eq(0).css("margin-top",0);
            $(".card.one-item.new-card-view").eq(1).css("margin-top",0);
            $(".hz-search #advanced-search-content").addClass("hidden");
            var intervalPosition = setInterval(function(){
                if ($(".hz-search #as-results-id_places__id__in").css("display") != "none"){
                    if (parseInt($(".hz-search #search-content .fields").eq(0).css("height")) > parseInt($(".hz-search #as-results-id_places__id__in").height())){
                        var a = $(".hz-search #search-content .fields").eq(0).css("height")
                        a = parseInt(a) - parseInt($(".hz-search #as-results-id_places__id__in").height());
                        if (a >= 70){
                        	$(".hz-search #search-content .fields").eq(0).css("height", a + "px");
                        }

                    }
                }else{
                    $(".hz-search #search-content .fields").eq(0).css("height", "100%");
                }

                $("#div_content_language").css("top",parseInt($("#search-button").eq(0).position().top) + 39 + "px");

                $("#div_content_language").css("left",$("#search-button").position().left + 20 + "px");
            }, 100)
            var momentcheck = setInterval(function(){
                if (window.moment){
                    clearInterval(momentcheck);
                }else{
                    return;
                }
            },500)
        })
    </script>



