<script type="text/javascript" charset="utf-8">
    function ninjaTrackingClickListener(e) {
        var eventName = $(this).attr('data-tr-event-name');
        var trackingObject = {};
        var trackingContext = $(this).attr('data-tr-context');
        if (trackingContext) {
            try {
                trackingContext = trackingContext.replace(/'/g, '"');
                trackingObject = JSON.parse(trackingContext);
            } catch (e) {
            }
        }
        trackingObject.trackEvent = [eventName];
        trackingObject.platformType = getPlatformType();
        if (e.target.href) {
            e.preventDefault();
            window.tracker.sendAnalyticsHit(trackingObject).then(function () {
                window.location.href = e.target.href;
            }).catch(function(ex){
                window.location.href = e.target.href;
            });
        } else {
            window.tracker.sendAnalyticsHit(trackingObject);
        }
        if (typeof appboy != "undefined" ) {
            appboy.logCustomEvent(eventName);
        }
    }
    try {

        (function waitjq(registerNinjaListener) {
            if (window.jQuery) {
                registerNinjaListener();
            } else {
                setTimeout(function () { waitjq(registerNinjaListener) }, 50);
            }
        })(registerNinjaListener);

        function registerNinjaListener() {
            $(document).on('click', "[data-tr-event-name]", ninjaTrackingClickListener);
        }

    } catch (err) {}
</script>

