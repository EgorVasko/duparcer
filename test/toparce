        // Initiailizing dbz variable incase it's not there when the
        // page is loaded or if network fails
        window.dbz = window.dbz || {};

        // safe_js: enables us to run any javascript code in guraded
        // environment, it'll evaluate the first argument which is always a
        // function and incase of exception print exceptions to console.
        window.dbz.safe_js = function(fn) {
            try {
                return fn();
            } catch (err) {
                if (console && console.log) {
                    console.log(err)
                }
            }
        };



	} // end init-site

