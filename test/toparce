    <script>

        function getSectionIcon(section_code){
    var iconURL = 'https://afs.googleusercontent.com/dubizzle/';
    switch (section_code){
        case 'MT':
            iconURL += 'motors-icon@2x.png';
            break;
        case 'SP':
            iconURL += 'p-4-s-icon@2x.png';
            break;
        case 'RP':
            iconURL += 'p-4-r-icon@2x.png';
            break;
        case 'JB':
            iconURL += 'jobs-icon@2x.png';
            break;
        case 'JW':
            iconURL += 'jobs-icon@2x.png';
            break;
        case 'CO':
            iconURL += 'community-icon@2x.png';
            break;
        default:
            iconURL += 'items-for-sale-icon@2x.png'
    }
    return iconURL;
}
(function() {
    var adIcon = getSectionIcon('MT');

    var basePageOptions = {
        'query': 'Motors Used Cars For Sale Bmw 1 Series Dubai United Arab Emirates',
        'pubId': 'partner-dubizzle-search',
        'adPage': '1',
        'location': false,
        'hl': 'en',
        'channel': 'motors+lpv',
        'sellerRatings': false,
        'linkTarget': '_blank',

        'adsafe': 'medium',
        'siteLinks': false,

    }
    var baseAdBlock = {
        'width': '740px',
        'colorTitleLink': '#3b4245',
        'colorText': '#626465',
        'colorDomainLink': '#E00000',
        'colorBackground': '#FFFFFF',
        'titleBold' : true,
        'detailedAttribution': false,
        'fontFamily' : 'roboto, arial, times new roman',
        'adIconUrl': adIcon,
        'adIconWidth': 184,
        'adIconHeight': 120,
        'adIconSpacingBefore': 0,
        'adIconSpacingAfter': 16,
        'domainLinkAboveDescription' : false,
        'clickableBackgrounds' : true,
        'noTitleUnderline': true,
        'fontSizeTitle' : 14,
        'fontSizeDescription' : 12,
        'fontSizeDomainLink' : 14,
