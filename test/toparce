      var userPath = getQueryParamValue('userpath');
      // To not overwrite when user path is already present
      if(userPath && !ninja_vars['userPath']){
        ninja_vars['userPath'] = userPath;
      }
      try {
        var _trackingInfo = getURLParameterByName('tracking_info');
        if (_trackingInfo){
          _trackingInfo = JSON.parse(decodeURIComponent(_trackingInfo));
          Object.keys(_trackingInfo).forEach(function(item){
            ninja_vars[item] = _trackingInfo[item];
          })
        }
      }catch(e){
          console.log("An Error Happened", e);
      }

