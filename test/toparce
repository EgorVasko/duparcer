  <button class="buy-car-widget__close js-buy-car-widget-close">
    &times;
  </button>
  <img class="buy-car-widget__img" src="https://dbzstatic-a.akamaihd.net/.dist_2e7af08869d35ada0997e4b2890c17a65184b4e6/images/dubizzle-pro@3x.png" alt="dubizzle pro" />
  <div class="buy-car-widget__wrapper">
    <p class="buy-car-widget__text">
      Save more than <em>AED 2,000</em> when <br/> buying your next car.
    </p>
    <a id="lnkProBuyCarConcierge" class="buy-car-widget__button js-buy-car-widget-button">
      Find out more
    </a>
  </div>
</div>

  <div class="buy-car-modal js-buy-car-modal">
    <div class="buy-car-modal__backdrop"></div>
    <div class="buy-car-modal__table">
      <div class="buy-car-modal__table-cell">
        <div class="buy-car-modal__card js-buy-car-widget-card">
          <button class="buy-car-modal__close js-buy-car-modal-close">
            &times;
          </button>
          <iframe class="buy-car-modal__iframe js-buy-car-modal-iframe" data-src="/motors/used-cars/pro/buy-car/"></iframe>
        </div>
      </div>
    </div>
  </div>

<script>
  (function() {
    var widget = document.querySelector('.js-buy-car-widget');
    var close = document.querySelector('.js-buy-car-widget-close');
    var lnkProBuyCarConcierge = document.getElementById('lnkProBuyCarConcierge');


      var button = document.querySelector('.js-buy-car-widget-button');
      var modal = document.querySelector('.js-buy-car-modal');
      var iframe = document.querySelector('.js-buy-car-modal-iframe');
      var modalClose = document.querySelector('.js-buy-car-modal-close');
      var card = document.querySelector('.js-buy-car-widget-card');


    // Modernizr's approach to check whether localStorage exists
    function isLocalStorage() {
      var test = 'test';
      try {
        localStorage.setItem(test, test);
        localStorage.removeItem(test);
        return true;
      } catch(e) {
        return false;
      }
    }

    // return and don’t show the widget if it hasn’t been dismissed within now and last week
    var aWeekAgo = new Date().setDate(new Date().getDate() - 7);
    if (
      isLocalStorage() &&
      localStorage.getItem('buy-car-widget-dismissed-at') &&
      parseInt(localStorage.getItem('buy-car-widget-dismissed-at'), 10) > aWeekAgo
    ) {
      return;
    }

    widget.removeAttribute('hidden');


      button.addEventListener('click', function(event) {
        event.preventDefault();
        modal.classList.add('is-active');

        // padding top
        var cardTop = card.getBoundingClientRect().top;

        // if the card’s height doesn’t fit into the window size
        // then reset the height to make it fit.
        if ((card.clientHeight + cardTop) > window.innerHeight) {
          card.style.height = window.innerHeight - (cardTop * 2) + 'px';
        }

        // only load iframe content when it’s active
        if (!iframe.src) {
          iframe.src = iframe.getAttribute('data-src');
        }
      });

      modalClose.addEventListener('click', function() {
        modal.classList.remove('is-active');
      });


    close.addEventListener('click', function() {
      widget.setAttribute('hidden', 'true');

      if (isLocalStorage()) {
        localStorage.setItem('buy-car-widget-dismissed-at', new Date().getTime());
      }
    });

    lnkProBuyCarConcierge.addEventListener('click', function() {
      window.tracker && window.tracker.sendAnalyticsHit({trackEvent: ['tap_more_info_dp_pg1']});

    });
  })();
</script>





