        <script type="text/javascript">
        function loadMediavoice(){
                if(window.topjsLoaded == true){
                    Modernizr.load([{
                        load: "//cdn.mediavoice.com/nativeads/script/dubizzle/mediavoice_uae_v2.js"
                    }]);
                }else{
                    setTimeout(function(){
                        loadMediavoice();
                    },200)
                }
        }
        loadMediavoice();
